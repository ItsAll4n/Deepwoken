repeat
    wait(1)
until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game:IsLoaded()

venyx = loadstring(game:HttpGet'https://raw.githubusercontent.com/zxciaz/VenyxUI/main/Reuploaded')()
ui = venyx.new('Midwoke', 5013109572)

themes = {
    Background = Color3.fromRGB(25, 25, 25),
    Glow = Color3.fromRGB(255, 255, 255),
    Accent = Color3.fromRGB(25, 25, 25),
