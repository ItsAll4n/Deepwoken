getgenv().nfall = false
getgenv().nclip = false
getgenv().fly = false
getgenv().hunger = false
getgenv().thirst = false
getgenv().closeplayer = false
getgenv().notify = false
getgenv().hnotify = false
getgenv().tnotify = false
getgenv().magpos = false
getgenv().bloop = false
getgenv().mcheck = false
getgenv().esp = false
flyspeed = 100

client = game:service'Players'.LocalPlayer
char = workspace.Live[client.Name] or client.Character or client.CharacterAdded:Wait(1)
hrp = char:WaitForChild'HumanoidRootPart'
mouse = client:GetMouse()
core = game:service'CoreGui'
cam = workspace.CurrentCamera
ctrl = {f = 0, b = 0, l = 0, r = 0}
lastctrl = {f = 0, b = 0, l = 0, r = 0}

function decode(str)
    local K, F = 8186484168865098, 16384 + 4887
    return (str:gsub('%x%x', function(c)
        local L = K % 274877906944
        local H = (K - L) / 274877906944
        local M = H % 128
        c = tonumber(c, 16)
        local m = (c + (H - M) / 128) * (2*M + 1) % 256
        K = L * F + H + c + m
        return string.char(m)
    end))
end

all4n = decode('146510be2c8e86c7')
lighting = game:service(all4n)

function unclip()
    pcall(function()
        char.Head.CanCollide = true
        char.Torso.CanCollide = true
        char['Left Leg'].CanCollide = false
        char['Right Leg'].CanCollide = false
    end)
end

function round(num, numDecimalPlaces)
	local mult = 10 ^ (numDecimalPlaces or 0)
	return math.floor(num * mult + 0.5) / mult
end

function getRoot(character)
	local rootPart = character:FindFirstChild'HumanoidRootPart' or character:FindFirstChild'Torso' or character:FindFirstChild'UpperTorso'
	return rootPart
end

function ESP(plr)
	task.spawn(function()
		for i,v in pairs(core:GetChildren()) do
			if v.Name == plr.Name..'_ESP' then
				v:Destroy()
			end
		end
		wait()
		if plr.Character and plr.Name ~= client.Name and not core:FindFirstChild(plr.Name..'_ESP') then
			local Holder = Instance.new'Folder'
			Holder.Name = plr.Name..'_ESP'
			Holder.Parent = core
			repeat wait(1) until plr.Character and getRoot(plr.Character) and plr.Character:FindFirstChildOfClass'Humanoid'
			for b,n in pairs (plr.Character:GetChildren()) do
				if n:IsA'BasePart' then
					local a = Instance.new'BoxHandleAdornment'
					a.Name = plr.Name
					a.Parent = Holder
					a.Adornee = n
					a.AlwaysOnTop = true
					a.ZIndex = 10
					a.Size = n.Size
					a.Transparency = 1
				end
			end
			if plr.Character and plr.Character:FindFirstChild'Head' then
				local Board = Instance.new'BillboardGui'
				local Label = Instance.new'TextLabel'
				Board.Adornee = plr.Character.Head
				Board.Name = plr.Name
				Board.Parent = Holder
				Board.Size = UDim2.new(0, 100, 0, 150)
				Board.StudsOffset = Vector3.new(0, 1, 0)
				Board.AlwaysOnTop = true
				Label.Parent = Board
				Label.BackgroundTransparency = 1
				Label.Position = UDim2.new(0, 0, 0, -50)
				Label.Size = UDim2.new(0, 100, 0, 100)
				Label.TextSize = 8
				Label.TextColor3 = Color3.new(255, 255, 255)
				Label.TextStrokeTransparency = 0
				Label.TextYAlignment = Enum.TextYAlignment.Bottom
				Label.Text = 'Name: '..plr.Name
				Label.ZIndex = 10
				local Loop
				local Added
				Added = plr.CharacterAdded:Connect(function()
					if esp then
						Loop:Disconnect()
						Holder:Destroy()
						repeat wait(1) until getRoot(plr.Character) and plr.Character:FindFirstChildOfClass'Humanoid'
						ESP(plr)
						Added:Disconnect()
					else
						Added:Disconnect()
					end
				end)
				local function espLoop()
					if core:FindFirstChild(plr.Name..'_ESP') then
						if plr.Character and getRoot(plr.Character) and plr.Character:FindFirstChildOfClass'Humanoid' and client.Character and getRoot(client.Character) and client.Character:FindFirstChildOfClass'Humanoid' then
							local pos = math.floor((getRoot(client.Character).Position - getRoot(plr.Character).Position).magnitude)
							Label.Text = plr.Name..' | Health: '..round(plr.Character:FindFirstChildOfClass'Humanoid'.Health, 1)..' | '..pos
						end
					else
						Added:Disconnect()
						Loop:Disconnect()
					end
				end
				Loop = game:service'RunService'.RenderStepped:Connect(espLoop)
			end
		end
	end)
end

function Tween(LOCATION)
    spawn(function()
        if game.PlaceId ~= 5735553160 and (hrp.Position - Vector3.new(LOCATION.Position.X, LOCATION.Position.Y, LOCATION.Position.Z)).magnitude > 200 then
            repeat
                wait()
                hrp.Position = hrp.Position - Vector3.new(0, 5, 0)
            until hrp.Position.Y <= 5
        end
        local check = Vector3.new(LOCATION.Position.X, LOCATION.Position.Y, LOCATION.Position.Z)
        if game.PlaceId ~= 5735553160 and (hrp.Position - Vector3.new(LOCATION.Position.X, LOCATION.Position.Y, LOCATION.Position.Z)).magnitude > 200 then
            check = Vector3.new(LOCATION.Position.X, 2, LOCATION.Position.Z)
        end
        repeat
            wait()
            local dir = check - hrp.Position
            hrp.Velocity = dir.unit * (math.random((250 - 1) * 1000, (250 + 2.5) * 1000) / 1000)
        until (hrp.Position - check).magnitude <= 10
        repeat
            wait()
            local dir = Vector3.new(LOCATION.Position.X, LOCATION.Position.Y, LOCATION.Position.Z) - hrp.Position
            hrp.Velocity = dir.unit * (math.random((200 - 1) * 1000, (200 + 2.5) * 1000) / 1000)
        until hrp.Position.Y >= LOCATION.Y
    end)
end

cp = ui:addPage('Client', 5012544693)
ps = cp:addSection('Personal')

ps:addKeybind('Fly', Enum.KeyCode.F4, function()
    if not fly then
        fly = true
        spawn(function()
            pcall(function()
                repeat wait()
                    if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then
                        bv = Instance.new('BodyVelocity', hrp)
                        bv.maxForce = Vector3.new(4000000, 4000000, 4000000)
                        bv.velocity = (cam.CoordinateFrame.lookVector * (ctrl.f + ctrl.b) + (cam.CoordinateFrame * CFrame.new(ctrl.l + ctrl.r, (ctrl.f + ctrl.b) * .2, 0).p) - cam.CoordinateFrame.p) * flyspeed / 1.5
                        hrp.Anchored = false
                        lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r}
                    else
                        hrp.Anchored = true
                    end
                until not fly
                hrp.Anchored = false
                ctrl = {f = 0, b = 0, l = 0, r = 0}
                lastctrl = {f = 0, b = 0, l = 0, r = 0}
            end)
        end)
    elseif fly then
        fly = false
    end
end)

ps:addKeybind('Noclip', Enum.KeyCode.F5, function()
    if not nclip then
        nclip = true
    elseif nclip then
        nclip = false
        unclip()
    end
end)

ps:addToggle('No Fall', nil, function(state)
    if not nfall then
        nfall = true
        local Closure,Caller = hide_me or newcclosure,checkcaller or is_protosmasher_caller or Cer.isCerus
        local call
        call = hookmetamethod(game,'__namecall', newcclosure(function(Self,...)
            local args = {...}
            if Caller() then return call(Self,...) end
            local method = getnamecallmethod()
            if method == 'FireServer' and nfall and typeof(args[1]) == 'number' then
                return
            elseif method == 'FindFirstChild' and args[1] == 'BodyVelocity' then
                return
            end
            return call(Self,...)
        end))
    elseif nfall then
        nfall = false
    end
end)

ps:addSlider('Speed',  100, 50, 200, function(speed)
    flyspeed = speed
end)

vp = ui:addPage('Visuals', 6523858394)
vs = vp:addSection('ESP')
vl = vp:addSection('Lighting')
vn = vp:addSection('Notify')
vc = vp:addSection('Stats')

vs:addToggle('Players', nil, function(eval)
    ESPenabled = eval
    if not ESPenabled then
        esp = false
        for i, c in pairs(core:GetChildren()) do
            if string.sub(c.Name, -4) == '_ESP' then
                c:Destroy()
            end
        end
    else
        esp = true
        for _, v in pairs(game.Players:GetChildren()) do
            if v.ClassName == 'Player' and v.Name ~= client.Name then
                ESP(v)
            end
        end
    end
end)

vl:addButton('Day', function()
    lighting.ClockTime = 14
end)

vl:addButton('Night', function()
    lighting.ClockTime = 0
end)

vl:addButton('Fog', function()
   lighting.FogEnd = 100000
	for i,v in pairs(lighting:GetDescendants()) do
		if v:IsA'Atmosphere' then
			v:Destroy()
		end
	end
end)

vn:addToggle('Mods', nil, function(state)
    if not mcheck then
        m = game:service'Players'.PlayerAdded:Connect(function(snitch)
            if snitch:IsInGroup(5212858) then
                ui:Notify(snitch.Name..'is a snitch!', 'I would leave if I were you.')
            end
        end)
    else
        m:Disconnect()
    end
end)

vn:addToggle('Players', nil, function(val)
    if not closeplayer then
        closeplayer = true
        while closeplayer do
            task.wait(1)
            for _, plr in pairs(game.Players:GetChildren()) do
                if plr.Name ~= client.Name and plr.Character and getRoot(plr.Character) and plr.Character:FindFirstChildOfClass'Humanoid' and char and getRoot(char) and char:FindFirstChildOfClass'Humanoid' then 
                    pos = math.floor((getRoot(char).Position - getRoot(plr.Character).Position).magnitude)
                    if pos <= 500 and not magpos and not notify then
                        ui:Notify(plr.Name..' is Near!', 'Current Distance: '..pos)
                        notify = true
                        magpos = true
                        task.wait()
                    end
                    if pos > 500 and magpos and not notify then
                        magpost = false
                    end
                    if pos > 500 and notify then
                        notify = false
                    end
                end
            end
        end
    elseif closeplayer then
        closeplayer = false
    end
end)

vn:addToggle('Hunger', nil, function(val)
    if not hunger then
        hunger = true
        char.Stomach:GetPropertyChangedSignal'Value':connect(function()
            if hunger and char.Stomach.Value <= 45 and not hnotify then
                hnotify = true
                ui:Notify('Hunger: Low', 'Eat Some Food!')
                wait(1)
            end
            if hunger and char.Stomach.Value > 45 and hnotify then
                hnotify = false
            end
        end)
    elseif hunger then
        hunger = false
    end
end)

vn:addToggle('Thirst', nil, function(val)
    if not thirst then
        thirst = true
        char.Water:GetPropertyChangedSignal'Value':connect(function()
            if thirst and char.Water.Value <= 45 and not tnotify then
                tnotify = true
                ui:Notify('Thirst: Low', 'Drink Some Water!')
                wait(1)
            end
            if thirst and char.Water.Value > 45 and tnotify then
                tnotify = false
            end
        end)
    elseif thirst then
        thirst = false
    end
end)

vc:addTextbox('Name:', 'Awaiting Input...', function(value, focusLost)
    name = value
    if (focusLost) then
    ui:Notify('Name: '..name, 'Ether: '..math.floor(workspace.Live[name].Ether.MaxValue)..' | Reservoir: '..math.floor(workspace.Live[name].Reservoir.MaxValue)..' | Posture: '..math.floor(workspace.Live[name].BreakMeter.Value)..' | Agility: '..math.floor(workspace.Live[name].Agility.Value)..' | Blood: '..math.floor(workspace.Live[name].Blood.Value))
    end
end)

