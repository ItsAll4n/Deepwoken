LS:addButton('Hide Fog', function()
    Lighting.FogEnd = 100000
    for i, effect in pairs(Lighting:GetDescendants()) do
		if effect:IsA'Atmosphere' then
			effect:Destroy()
		end
	end
end)

NS:addToggle('Mods', nil, function(state)
    if not Mods then
        Mods = true
        for _, snitch in pairs(game:GetService'Players':GetChildren()) do
            if snitch:IsInGroup(5212858) then
                game:GetService'StarterGui':SetCore('SendNotification', {
                    Title = snitch.Name..' Is a Snitch!',
                    Text = 'Would you like to leave?',
                    Duration = 100,
                    Callback = data,
                    Button1 = 'Yes',
                    Button2 = 'No'
                })
            end
        end
        Check = game:GetService'Players'.PlayerAdded:Connect(function(snitch)
            if snitch:IsInGroup(5212858) then
                game:GetService'StarterGui':SetCore('SendNotification', {
                    Title = snitch.Name..' Is a Snitch!',
                    Text = 'Would you like to leave?',
                    Duration = 100,
                    Callback = data,
                    Button1 = 'Yes',
                    Button2 = 'No'
                })
            end
        end)
    else
        Mods = false
        Check:Disconnect()
    end
end)

NS:addToggle('Players', nil, function(val)
    if not Players then
        Players = true
        while Players do
            task.wait(1)
            for _, plr in pairs(game.Players:GetChildren()) do
                if plr.Name ~= client.Name and plr.Character and Root(plr.Character) and plr.Character:FindFirstChildOfClass'Humanoid' and char and Root(char) and char:FindFirstChildOfClass'Humanoid' then 
                    pos = math.floor((Root(char).Position - Root(plr.Character).Position).magnitude)
                    if pos <= 500 and not Notify then
                        UI:Notify(plr.Name..' is Near!', 'Current Distance: '..pos)
                        Notify = true
                        task.wait(1)
                    end
                    if pos > 500 and Notify then
                        Notify = false
                    end
                end
            end
        end
    else
        Players = false
        Notify = false
    end
end)

NS:addToggle('Hunger', nil, function(val)
    if not Hunger then
        Hunger = true
        char.Stomach:GetPropertyChangedSignal'Value':connect(function()
            if Hunger and char.Stomach.Value <= 45 and not HNotify then
                HNotify = true
                UI:Notify('Hunger: Low', 'Eat Some Food!')
                wait(1)
            end
            if Hunger and char.Stomach.Value > 45 and HNotify then
                HNotify = false
            end
        end)
    else
        Hunger = false
        HNotify = false
    end
end)

NS:addToggle('Thirst', nil, function(val)
    if not Thirst then
        Thirst = true
        char.Water:GetPropertyChangedSignal'Value':connect(function()
            if Thirst and char.Water.Value <= 45 and not TNotify then
                TNotify = true
                UI:Notify('Thirst: Low', 'Drink Some Water!')
                wait(1)
            end
            if Thirst and char.Water.Value > 45 and TNotify then
                TNotify = false
            end
        end)
    else
        Thirst = false
        TNotify = false
    end
end)

SS:addTextbox('Name:', 'Awaiting Input...', function(value, focusLost)
    name = value
    if (focusLost) then
    UI:Notify('Name: '..name, 'Ether: '..math.floor(workspace.Live[name].Ether.MaxValue)..' | Reservoir: '..math.floor(workspace.Live[name].Reservoir.MaxValue)..' | Posture: '..math.floor(workspace.Live[name].BreakMeter.Value)..' | Agility: '..math.floor(workspace.Live[name].Agility.Value)..' | Blood: '..math.floor(workspace.Live[name].Blood.Value))
    end
end)

