    TS:addColorPicker(theme, color, function(color3)
        UI:setTheme(theme, color3)
    end)
end

game:GetService'RunService'.Stepped:connect(function()
    pcall(function()
        if Noclip then
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA'BasePart' then
                    part.CanCollide = false
                end
            end
        end
    end)
end)

game:GetService'Players'.PlayerRemoving:Connect(function(player)
    if Esp then
        for _, ui in pairs(core:GetChildren()) do
            if ui.Name == player.Name..'_ESP' then
                ui:Destroy()
            end
        end
    end
end)

game:GetService'Players'.PlayerAdded:Connect(function(player)
    if Esp then
        repeat wait(1) until player.Character and Root(player.Character)
		ESP(player)
    end
end)

workspace.Live.ChildRemoved:Connect(function(enemy)
    if EnemyEsp then
        for _, ui in pairs(core:GetChildren()) do
            if ui.Name == enemy.Name..'!ESP' then
                ui:Destroy()
            end
        end
    end
end)

workspace.Live.ChildAdded:Connect(function(enemy)
    if EnemyEsp then
        if enemy:WaitForChild'SpawnCF' then
            repeat wait(1) until enemy and Root(enemy)
            EnemyESP(enemy)
        end
    end
end)

mouse.KeyDown:connect(function(key)
    if key:lower():match'w' then
        ctrl.f = 1
    elseif key:lower():match's' then
        ctrl.b = -1
    elseif key:lower():match'a' then
        ctrl.l = -1
    elseif key:lower():match'd' then
        ctrl.r = 1
    end
end)

mouse.KeyUp:connect(function(key)
    if key:lower():match'w' then
        ctrl.f = 0
    elseif key:lower():match's' then
        ctrl.b = 0
    elseif key:lower():match'a' then
        ctrl.l = 0
    elseif key:lower():match'd' then
        ctrl.r = 0
    end
end)
