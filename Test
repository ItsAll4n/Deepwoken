            DropDownButton.Position = UDim2.new(0.51509434, 0, 0.210526243, 0)
            DropDownButton.Size = UDim2.new(0, 227, 0, 25)
            DropDownButton.Font = Enum.Font.SourceSans
            DropDownButton.Text = ""
            DropDownButton.TextColor3 = Color3.fromRGB(0, 0, 0)
            DropDownButton.TextSize = 14.000

            DropDownDesc.Name = "DropDownDesc"
            DropDownDesc.Parent = DropDownTitle
            DropDownDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropDownDesc.BackgroundTransparency = 1.000
            DropDownDesc.Position = UDim2.new(0.00769231096, 0, 0.720000029, 0)
            DropDownDesc.Size = UDim2.new(0, 190, 0, 20)
            DropDownDesc.Font = Enum.Font.Gotham
            DropDownDesc.Text = Desc
            DropDownDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            DropDownDesc.TextSize = 10.000
            DropDownDesc.TextXAlignment = Enum.TextXAlignment.Left

            DropDownCorner.Name = "DropDownCorner"
            DropDownCorner.Parent = DropDown

            DisplayText.Name = "DisplayText"
            DisplayText.Parent = DropDown
            DisplayText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DisplayText.BackgroundTransparency = 1.000
            DisplayText.Position = UDim2.new(0.53018868, 0, 0.210526317, 0)
            DisplayText.Size = UDim2.new(0, 217, 0, 23)
            DisplayText.Font = Enum.Font.Gotham
            DisplayText.Text = "..."
            DisplayText.TextColor3 = Color3.fromRGB(255, 255, 255)
            DisplayText.TextSize = 14.000
            DisplayText.TextXAlignment = Enum.TextXAlignment.Left

            Underbar.Name = "Underbar"
            Underbar.Parent = DisplayText
            Underbar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Underbar.BorderSizePixel = 0
            Underbar.Position = UDim2.new(-0.0194678903, 0, 1.0053395, 0)
            Underbar.Size = UDim2.new(0, 225, 0, 1)

            format_list_bulleted.Name = "format_list_bulleted"
            format_list_bulleted.Parent = DropDown
            format_list_bulleted.BackgroundTransparency = 1.000
            format_list_bulleted.Position = UDim2.new(0.45094341, 0, 0.236842126, 0)
            format_list_bulleted.Size = UDim2.new(0, 25, 0, 25)
            format_list_bulleted.ZIndex = 2
            format_list_bulleted.Image = "rbxassetid://3926305904"
            format_list_bulleted.ImageRectOffset = Vector2.new(484, 204)
            format_list_bulleted.ImageRectSize = Vector2.new(36, 36)

            DropDownButton.MouseButton1Click:Connect(function()
                if DropTog == false then
                    DropDownFrame.Visible = true
                    DropDown.Size = UDim2.new(0, 530, 0, 145)
                    Page.CanvasSize = UDim2.new(0,0,0, List.AbsoluteContentSize.Y)
                else
                    DropDownFrame.Visible = false
                    Page.CanvasSize = UDim2.new(0,0,0, List.AbsoluteContentSize.Y)
                end
                DropTog = not DropTog
            end)
            for i,v in next, list do
                itemcount = itemcount + 1
                
                if itemcount == 1 then
                    framesize = 29
                elseif itemcount == 2 then
                    framesize = 58
                elseif itemcount >= 3 then
                    framesize = 87
                end
                
                local Item = Instance.new("TextButton")
                local ItemText = Instance.new("TextLabel")
                local ItemCorner = Instance.new("UICorner")

                Item.Name = "Item"
                Item.Parent = DropDownFrame
                Item.BackgroundColor3 = Color3.fromRGB(215, 215, 215)
                Item.BorderSizePixel = 0
                Item.Position = UDim2.new(0.00411522621, 0, 0, 0)
                Item.Size = UDim2.new(0, 464, 0, 23)
                Item.Font = Enum.Font.SourceSans
                Item.Text = ""
                Item.TextColor3 = Color3.fromRGB(0, 0, 0)
                Item.TextSize = 14.000
                Item.AutoButtonColor = false
                Item.BackgroundTransparency = 1.000

                ItemText.Name = "ItemText"
                ItemText.Parent = Item
                ItemText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                ItemText.BackgroundTransparency = 1.000
                ItemText.BorderSizePixel = 0
                ItemText.Position = UDim2.new(0.580804527, 0, 0.0329828635, 0)
                ItemText.Size = UDim2.new(0, 185, 0, 22)
                ItemText.Font = Enum.Font.Gotham
                ItemText.TextColor3 = Color3.fromRGB(255, 255, 255)
                ItemText.TextSize = 14.000
                ItemText.TextXAlignment = Enum.TextXAlignment.Right
                ItemText.Text = v

                ItemCorner.Name = "ItemCorner"
                ItemCorner.Parent = Item
                
                Item.MouseEnter:Connect(function()
                    ItemText.TextColor3 = Color3.fromRGB(255,255,255)
                    Item.BackgroundTransparency = 0
                end)
                
                Item.MouseLeave:Connect(function()
                    ItemText.TextColor3 = Color3.fromRGB(232, 232, 232)
                    Item.BackgroundTransparency = 1
                end)
                
                Item.MouseButton1Click:Connect(function()
                    DisplayText.Text = v
                    pcall(callback, v)
                    DropDown.Size = UDim2.new(0, 530, 0, 57)
                    DropDownFrame.Visible = false
                    Page.CanvasSize = UDim2.new(0,0,0, List.AbsoluteContentSize.Y)
                    DropTog = not DropTog
                end)
                
                DropDownFrame.CanvasSize = UDim2.new(0,0,0,DropDownFrameListLayOut.AbsoluteContentSize.Y)
                
                DropDownFrame.Size = UDim2.new(0, 217, 0, framesize)
                DropDownFrame.Size = UDim2.new(0, 217, 0, framesize + 6)
            end
            DropDownFrame.Size = UDim2.new(0, 486, 0, 74)
            Page.CanvasSize = UDim2.new(0,0,0, List.AbsoluteContentSize.Y)
        end
        function Content:TextBox(Title, Desc, overlay, callback)
            local callback = callback or function() end
            overlay = overlay or "..."
            local TextBox = Instance.new("Frame")
            local TextBoxCorner = Instance.new("UICorner")
            local TextBoxTitle = Instance.new("TextLabel")
            local TextBoxDesc = Instance.new("TextLabel")
            local TextBox_2 = Instance.new("TextBox")
            local UnderBar = Instance.new("Frame")
            local rate_review = Instance.new("ImageButton")

            TextBox.Name = "TextBox"
            TextBox.Parent = Page
            TextBox.BackgroundColor3 = Color3.fromRGB(180, 180, 180)
            TextBox.BorderSizePixel = 0
            TextBox.Position = UDim2.new(-0.0928961784, 0, 0.636978328, 0)
            TextBox.Size = UDim2.new(0, 530, 0, 57)

            TextBoxCorner.Name = "TextBoxCorner"
            TextBoxCorner.Parent = TextBox

            TextBoxTitle.Name = "TextBoxTitle"
            TextBoxTitle.Parent = TextBox
            TextBoxTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxTitle.BackgroundTransparency = 1.000
            TextBoxTitle.Position = UDim2.new(0.0245283023, 0, 0.122000001, 0)
            TextBoxTitle.Size = UDim2.new(0, 130, 0, 25)
            TextBoxTitle.Font = Enum.Font.Gotham
            TextBoxTitle.Text = Title
            TextBoxTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxTitle.TextSize = 12.000
            TextBoxTitle.TextXAlignment = Enum.TextXAlignment.Left

            TextBoxDesc.Name = "TextBoxDesc"
            TextBoxDesc.Parent = TextBoxTitle
            TextBoxDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxDesc.BackgroundTransparency = 1.000
            TextBoxDesc.Position = UDim2.new(0.00769231096, 0, 0.720000029, 0)
            TextBoxDesc.Size = UDim2.new(0, 190, 0, 20)
            TextBoxDesc.Font = Enum.Font.Gotham
            TextBoxDesc.Text = Desc
            TextBoxDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxDesc.TextSize = 10.000
            TextBoxDesc.TextXAlignment = Enum.TextXAlignment.Left

            TextBox_2.Parent = TextBox
            TextBox_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextBox_2.BackgroundTransparency = 1.000
            TextBox_2.Position = UDim2.new(0.54905659, 0, 0.121999644, 0)
            TextBox_2.Size = UDim2.new(0, 190, 0, 27)
            TextBox_2.Font = Enum.Font.Gotham
            TextBox_2.PlaceholderColor3 = Color3.fromRGB(255, 255, 255)
            TextBox_2.PlaceholderText = overlay or "..."
            TextBox_2.Text = ""
            TextBox_2.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextBox_2.TextSize = 14.000
            TextBox_2.TextXAlignment = Enum.TextXAlignment.Left

            UnderBar.Name = "UnderBar"
            UnderBar.Parent = TextBox_2
            UnderBar.BackgroundColor3 = Color3.fromRGB(206, 206, 206)
            UnderBar.BorderSizePixel = 0
            UnderBar.Position = UDim2.new(-0.0536620915, 0, 0.966054201, 0)
            UnderBar.Size = UDim2.new(0, 199, 0, 1)

            rate_review.Name = "rate_review"
            rate_review.Parent = TextBox_2
            rate_review.BackgroundTransparency = 1.000
            rate_review.Position = UDim2.new(-0.273684204, 0, 0.314814806, 0)
            rate_review.Size = UDim2.new(0, 25, 0, 25)
            rate_review.ZIndex = 2
            rate_review.Image = "rbxassetid://3926307971"
            rate_review.ImageRectOffset = Vector2.new(124, 404)
            rate_review.ImageRectSize = Vector2.new(36, 36)

            TextBox_2.Focused:Connect(function()
                TweenService:Create(UnderBar, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundColor3 = Color3.fromRGB(255,255,255)}):Play()
            end)
            TextBox_2.FocusLost:Connect(function()
                TweenService:Create(UnderBar, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundColor3 = Color3.fromRGB(206, 206, 206)}):Play()
            end)

            TextBox_2.FocusLost:Connect(function(enter)
                if enter then
                    if #TextBox_2.Text > 0 then
                        pcall(callback, TextBox_2.Text)
                    end
                end
            end)
        end
        return Content
    end
    return NewContent
end
return Lib
