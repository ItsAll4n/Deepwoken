            ToggleDesc.Font = Enum.Font.Gotham
            ToggleDesc.Text = Desc
            ToggleDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            ToggleDesc.TextSize = 10.000
            ToggleDesc.TextXAlignment = Enum.TextXAlignment.Left

            ToggleCorner.Name = "ToggleCorner"
            ToggleCorner.Parent = Toggle

            Toggle_2.Name = "Toggle"
            Toggle_2.Parent = Toggle
            Toggle_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Toggle_2.BackgroundTransparency = 1.000
            Toggle_2.Position = UDim2.new(0.909359694, -23, 0.5, -11)
            Toggle_2.Size = UDim2.new(0, 46, 0, 22)
            Toggle_2.Image = "rbxassetid://3570695787"
            Toggle_2.ImageColor3 = Color3.fromRGB(38, 38, 38)
            Toggle_2.ScaleType = Enum.ScaleType.Slice
            Toggle_2.SliceCenter = Rect.new(100, 100, 100, 100)
            Toggle_2.SliceScale = 0.120

            ToggleButton.Name = "ToggleButton"
            ToggleButton.Parent = Toggle_2
            ToggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleButton.BackgroundTransparency = 1.000
            ToggleButton.Size = UDim2.new(1, 0, 1, 0)
            ToggleButton.Font = Enum.Font.SourceSans
            ToggleButton.TextColor3 = Color3.fromRGB(0, 0, 0)
            ToggleButton.TextSize = 14.000
            ToggleButton.TextTransparency = 1.000

            ToggleCircle.Name = "ToggleCircle"
            ToggleCircle.Parent = Toggle_2
            ToggleCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleCircle.BackgroundTransparency = 1.000
            ToggleCircle.Position = UDim2.new(0.221739143, -5, 0.0970000029, 0)
            ToggleCircle.Size = UDim2.new(0, 18, 0, 18)
            ToggleCircle.Image = "rbxassetid://3570695787"
            ToggleCircle.ScaleType = Enum.ScaleType.Slice
            ToggleCircle.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleCircle.SliceScale = 0.120

            ToggleButton.MouseButton1Click:Connect(function()
                if toggled == false then
                    TweenService:Create(
                        Toggle_2,
                        TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                        {ImageColor3 = Color3.fromRGB(85, 0, 204)}
                    ):Play()
                    ToggleCircle:TweenPosition(UDim2.new(0.602, -5,0.097, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, .3, true)
                    wait(.1)
                else
                    TweenService:Create(
                        Toggle_2,
                        TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                        {ImageColor3 = Color3.fromRGB(38, 38, 38)}
                    ):Play()
                    ToggleCircle:TweenPosition(UDim2.new(0.222, -5,0.097, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, .3, true)
                end
                toggled = not toggled
                pcall(callback, toggled)
            end)
            Page.CanvasSize = UDim2.new(0,0,0, List.AbsoluteContentSize.Y)
        end
        function Content:Slider(Title, Desc, min, max, callback)
            min = min or 0
            max = max or 100
            local mouse = game:service'Players'.LocalPlayer:GetMouse()
            local uis = game:service'UserInputService'
            local Value;
            local callback = callback or function() end
            local Slider = Instance.new("Frame")
            local SliderTitle = Instance.new("TextLabel")
            local SliderDesc = Instance.new("TextLabel")
            local SliderCorner = Instance.new("UICorner")
            local SliderOutter = Instance.new("TextButton")
            local SliderOutterCorner = Instance.new("UICorner")
            local Inner = Instance.new("TextLabel")
            local InnerCorner = Instance.new("UICorner")
            local ValueLabel = Instance.new("TextLabel")
            local show_chart = Instance.new("ImageButton")

            --Properties:

            Slider.Name = "Slider"
            Slider.Parent = Page
            Slider.BackgroundColor3 = Color3.fromRGB(180, 180, 180)
            Slider.BorderSizePixel = 0
            Slider.Position = UDim2.new(0.0255009104, 0, 0.298076928, 0)
            Slider.Size = UDim2.new(0, 530, 0, 57)

            SliderTitle.Name = "SliderTitle"
            SliderTitle.Parent = Slider
            SliderTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderTitle.BackgroundTransparency = 1.000
            SliderTitle.Position = UDim2.new(0.0245283023, 0, 0.122000001, 0)
            SliderTitle.Size = UDim2.new(0, 130, 0, 25)
            SliderTitle.Font = Enum.Font.Gotham
            SliderTitle.Text = Title
            SliderTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            SliderTitle.TextSize = 12.000
            SliderTitle.TextXAlignment = Enum.TextXAlignment.Left

            SliderDesc.Name = "SliderDesc"
            SliderDesc.Parent = SliderTitle
            SliderDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderDesc.BackgroundTransparency = 1.000
            SliderDesc.Position = UDim2.new(0.00769231096, 0, 0.720000029, 0)
            SliderDesc.Size = UDim2.new(0, 190, 0, 20)
            SliderDesc.Font = Enum.Font.Gotham
            SliderDesc.Text = Desc
            SliderDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            SliderDesc.TextSize = 10.000
            SliderDesc.TextXAlignment = Enum.TextXAlignment.Left

            SliderCorner.Name = "SliderCorner"
            SliderCorner.Parent = Slider

            SliderOutter.Name = "SliderOutter"
            SliderOutter.Parent = Slider
            SliderOutter.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderOutter.BorderSizePixel = 0
            SliderOutter.Position = UDim2.new(0.51509434, 0, 0.455333471, 0)
            SliderOutter.Size = UDim2.new(0, 242, 0, 8)
            SliderOutter.AutoButtonColor = false
            SliderOutter.Text = ""


            SliderOutterCorner.Name = "SliderOutterCorner"
            SliderOutterCorner.Parent = SliderOutter

            Inner.Name = "Inner"
            Inner.Parent = SliderOutter
            Inner.BackgroundColor3 = Color3.fromRGB(85, 0, 204)
            Inner.Size = UDim2.new(0, 131, 0, 8)
            Inner.Position = UDim2.new(0, -1, 0, 0)
            Inner.BorderSizePixel = 0
            Inner.Text = ""

            InnerCorner.Name = "InnerCorner"
            InnerCorner.Parent = Inner

            ValueLabel.Name = "ValueLabel"
            ValueLabel.Parent = Slider
            ValueLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ValueLabel.BackgroundTransparency = 1.000
            ValueLabel.Position = UDim2.new(0.792452812, 0, 0.122000173, 0)
            ValueLabel.Size = UDim2.new(0, 104, 0, 17)
            ValueLabel.Font = Enum.Font.Gotham
            ValueLabel.Text = ""
            ValueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            ValueLabel.TextSize = 14.000

            show_chart.Name = "show_chart"
            show_chart.Parent = Slider
            show_chart.BackgroundTransparency = 1.000
            show_chart.Position = UDim2.new(0.45094341, 0, 0.289473683, 0)
            show_chart.Size = UDim2.new(0, 25, 0, 25)
            show_chart.ZIndex = 2
            show_chart.Image = "rbxassetid://3926307971"
            show_chart.ImageRectOffset = Vector2.new(404, 164)
            show_chart.ImageRectSize = Vector2.new(36, 36)

            SliderOutter.MouseButton1Down:Connect(function()
                Value = math.floor((((tonumber(max) - tonumber(min)) / 244) * Inner.AbsoluteSize.X) + tonumber(min)) or 0
                pcall(function()
                callback(Value)
            end)
            Inner.Size = UDim2.new(0, math.clamp(mouse.X - Inner.AbsolutePosition.X, 0, 244), 0, 8)
            moveconnection = mouse.Move:Connect(function()
                ValueLabel.Text = Value
                Value = math.floor((((tonumber(max) - tonumber(min)) / 244) * Inner.AbsoluteSize.X) + tonumber(min))
                pcall(function()
                    callback(Value)
                end)
                Inner.Size = UDim2.new(0, math.clamp(mouse.X - Inner.AbsolutePosition.X, 0, 244), 0, 8)
            end)
            releaseconnection = uis.InputEnded:Connect(function(Mouse)
                if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                    Value = math.floor((((tonumber(max) - tonumber(min)) / 244) * Inner.AbsoluteSize.X) + tonumber(min))
                    pcall(function()
                        callback(Value)
                    end)
                    Inner.Size = UDim2.new(0, math.clamp(mouse.X - Inner.AbsolutePosition.X, 0, 244), 0, 8)
                    moveconnection:Disconnect()
                    releaseconnection:Disconnect()
                end
            end)
            end)
            Page.CanvasSize = UDim2.new(0,0,0, List.AbsoluteContentSize.Y)
        end
        function Content:DropDown(Title, Desc, list, callback)
