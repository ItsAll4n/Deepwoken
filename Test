    function Lib:NewTask(Title, Desc, CC1, CC2, CC3, TrackDate, TrackTime, TrackLevel)
        local Clr1 = CC1 or 255
        local Clr2 = CC2 or 255
        local Clr3 = CC3 or 255
        local TrackDate = TrackDate or false
        local TrackTime = TrackTime or false
        local TrackLevel = TrackLevel or false
        local Task = Instance.new("Frame")
        local TaskCorner = Instance.new("UICorner")
        local TaskTitle = Instance.new("TextLabel")
        local TaskDesc = Instance.new("TextLabel")
        local TaskViewButton = Instance.new("TextButton")
        local UICorner = Instance.new("UICorner")
        local TaskGradient = Instance.new("UIGradient")
        if TrackDate == true then
            event_busy = Instance.new("ImageButton")
            StartDesc = Instance.new("TextLabel")

            event_busy.Name = "event_busy"
            event_busy.Parent = CurrentTaskDisplay
            event_busy.BackgroundTransparency = 1.000
            event_busy.LayoutOrder = 10
            event_busy.Position = UDim2.new(0.0666666627, 0, 0.440625012, 0)
            event_busy.Size = UDim2.new(0, 25, 0, 25)
            event_busy.ZIndex = 2
            event_busy.Image = "rbxassetid://3926305904"
            event_busy.ImageColor3 = Color3.fromRGB(125, 125, 125)
            event_busy.ImageRectOffset = Vector2.new(564, 84)
            event_busy.ImageRectSize = Vector2.new(36, 36)

            StartDesc.Name = "StartDesc"
            StartDesc.Parent = event_busy
            StartDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            StartDesc.BackgroundTransparency = 1.000
            StartDesc.Position = UDim2.new(1.27999997, 0, 0.54611814, 0)
            StartDesc.Size = UDim2.new(0, 100, 0, 11)
            StartDesc.Font = Enum.Font.Gotham
            StartDesc.Text = "Started At:"
            StartDesc.TextColor3 = Color3.fromRGB(115, 115, 115)
            StartDesc.TextSize = 9.000
            StartDesc.TextXAlignment = Enum.TextXAlignment.Left
        end
        if TrackTime == true then
            timer = Instance.new("ImageButton")
            timerdesc = Instance.new("TextLabel")

            timer.Name = "timer"
            timer.Parent = CurrentTaskDisplay
            timer.BackgroundTransparency = 1.000
            timer.LayoutOrder = 5
            timer.Position = UDim2.new(0.0666666627, 0, 0.634375036, 0)
            timer.Size = UDim2.new(0, 25, 0, 25)
            timer.ZIndex = 2
            timer.Image = "rbxassetid://3926305904"
            timer.ImageColor3 = Color3.fromRGB(125, 125, 125)
            timer.ImageRectOffset = Vector2.new(704, 4)
            timer.ImageRectSize = Vector2.new(36, 36)

            timerdesc.Name = "timerdesc"
            timerdesc.Parent = timer
            timerdesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            timerdesc.BackgroundTransparency = 1.000
            timerdesc.Position = UDim2.new(1.39999998, 0, 0.400000006, 0)
            timerdesc.Size = UDim2.new(0, 100, 0, 18)
            timerdesc.Font = Enum.Font.SourceSans
            timerdesc.Text = "Duration"
            timerdesc.TextColor3 = Color3.fromRGB(115, 115, 115)
            timerdesc.TextSize = 11.000
            timerdesc.TextXAlignment = Enum.TextXAlignment.Left
        end
        if TrackLevel == true then
            levels = Instance.new("ImageButton")
            leveldesc = Instance.new("TextLabel")

            levels.Name = "levels"
            levels.Parent = CurrentTaskDisplay
            levels.BackgroundTransparency = 1.000
            levels.LayoutOrder = 10
            levels.Position = UDim2.new(0.0666666627, 0, 0.53125, 0)
            levels.Size = UDim2.new(0, 25, 0, 25)
            levels.ZIndex = 2
            levels.Image = "rbxassetid://3926305904"
            levels.ImageColor3 = Color3.fromRGB(125, 125, 125)
            levels.ImageRectOffset = Vector2.new(604, 364)
            levels.ImageRectSize = Vector2.new(36, 36)

            leveldesc.Name = "leveldesc"
            leveldesc.Parent = levels
            leveldesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            leveldesc.BackgroundTransparency = 1.000
            leveldesc.Position = UDim2.new(1.39999998, 0, 0.439999998, 0)
            leveldesc.Size = UDim2.new(0, 100, 0, 13)
            leveldesc.Font = Enum.Font.GothamSemibold
            leveldesc.Text = "Levels Gained"
            leveldesc.TextColor3 = Color3.fromRGB(115, 115, 115)
            leveldesc.TextSize = 9.000
            leveldesc.TextXAlignment = Enum.TextXAlignment.Left
        end
        Task.Name = Title.."Task"
        Task.Parent = CurrentTaskScrollFrame
        Task.BackgroundColor3 = Color3.fromRGB(230, 230, 230)
        Task.Position = UDim2.new(0.420765042, 0, 0.0600961521, 0)
        Task.Size = UDim2.new(0, 120, 0, 100)

        TaskCorner.Name = "TaskCorner"
        TaskCorner.Parent = Task

        TaskTitle.Name = "TaskTitle"
        TaskTitle.Parent = Task
        TaskTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TaskTitle.BackgroundTransparency = 1.000
        TaskTitle.Position = UDim2.new(0.108333342, 0, 0.0299999993, 0)
        TaskTitle.Size = UDim2.new(0, 95, 0, 29)
        TaskTitle.Font = Enum.Font.GothamSemibold
        TaskTitle.Text = Title
        TaskTitle.TextColor3 = Color3.fromRGB(0, 0, 0)
        TaskTitle.TextSize = 12.000
        TaskTitle.TextXAlignment = Enum.TextXAlignment.Left

        TaskDesc.Name = "TaskDesc"
        TaskDesc.Parent = TaskTitle
        TaskDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        TaskDesc.BackgroundTransparency = 1.000
        TaskDesc.Position = UDim2.new(0, 0, 0.793103456, 0)
        TaskDesc.Size = UDim2.new(0, 105, 0, 18)
        TaskDesc.Font = Enum.Font.Gotham
        TaskDesc.Text = "Inactive"
        TaskDesc.TextColor3 = Color3.fromRGB(115, 115, 115)
        TaskDesc.TextSize = 10.000
        TaskDesc.TextXAlignment = Enum.TextXAlignment.Left

        TaskViewButton.Name = "TaskViewButton"
        TaskViewButton.Parent = Task
        TaskViewButton.BackgroundColor3 = Color3.fromRGB(85, 0, 255)
        TaskViewButton.BorderSizePixel = 0
        TaskViewButton.Position = UDim2.new(0.100000001, 0, 0.547700167, 0)
        TaskViewButton.Size = UDim2.new(0, 95, 0, 28)
        TaskViewButton.Font = Enum.Font.Gotham
        TaskViewButton.Text = "View"
        TaskViewButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TaskViewButton.TextSize = 14.000

        TaskViewButton.MouseButton1Click:connect(function()
            CurrentTaskDisplayTitle.Text = Title
            CurrentTaskDisplayBody.Text = Desc
        end)

        UICorner.Parent = TaskViewButton

        TaskGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 255, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(Clr1, Clr2, Clr3))}
        TaskGradient.Rotation = 90
        TaskGradient.Name = "TaskGradient"
        TaskGradient.Parent = Task

        CurrentTaskScrollFrame.CanvasSize = UDim2.new(0,0,0, CurrentTaskScrollFrameListLayout.AbsoluteContentSize.Y)
    end

    Run.Heartbeat:connect(function()
        Date.Text = os.date(currentdate)
        Time.Text = os.date(currenttime)
        local seconds = math.floor(workspace.DistributedGameTime)
        local minutes = math.floor(workspace.DistributedGameTime / 60)
        local hours = math.floor(workspace.DistributedGameTime / 60 / 60)
        local seconds = seconds - (minutes * 60)
        local minutes = minutes - (hours * 60)
        if hours < 1 then if minutes < 1 then
            CurrentSessionTime.Text = "Current Session Time: " ..seconds .. " " else
                CurrentSessionTime.Text = "Current Session Time: " .. minutes .. ":" .. seconds .. " "
        end
        else
		    CurrentSessionTime.Text = "Current Session Time: " ..hours .. ":" .. minutes .. ":" .. seconds .. " "
            wait(1)
        end
    end)
    clear.MouseEnter:connect(function()
        Close.BackgroundTransparency = 0 
    end)
    clear.MouseLeave:connect(function()
        Close.BackgroundTransparency = 1
    end)
    clear.MouseButton1Click:connect(function()
        game.CoreGui.Hold:Destroy()
    end)

    Moon.MouseEnter:connect(function()
        DarkMode.BackgroundTransparency = 0
    end)
    Moon.MouseLeave:connect(function()
        DarkMode.BackgroundTransparency = 1
    end)
    Moon.MouseButton1Click:connect(function()
        for i, v in pairs(game.CoreGui.Hold:GetChildren()) do
            if v.ClassName == "Frame" and v.BackgroundColor3 == Color3.fromRGB(255, 255, 255) then
                TweenService:Create(v, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
            end
        end
        for m, x in pairs(game.CoreGui.Hold.Main.Dashboard:GetChildren()) do
            if x.ClassName == "Frame" and x.BackgroundColor3 == Color3.fromRGB(240,240,240) then
                TweenService:Create(x, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
            end
            for l , m in pairs(x:GetDescendants()) do
                if m.ClassName == "TextLabel" and m.TextColor3 == Color3.fromRGB(0, 0, 0) then
                    TweenService:Create(m, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                end
            end
        end
        for i, v in pairs(game.CoreGui.Hold.Main.TabContentHolder.TabsFolder:GetDescendants()) do
            if v.ClassName == "Frame" or v.ClassName == "TextButton" and v.BackgroundColor3 == Color3.fromRGB(180, 180, 180) then
                v.BackgroundColor3 = Color3.fromRGB(44, 44, 44)
            end
        end
    end)

    Sun.MouseEnter:connect(function()
        LightMode.BackgroundTransparency = 0
    end)
    Sun.MouseLeave:connect(function()
        LightMode.BackgroundTransparency = 1
    end)
    Sun.MouseButton1Click:connect(function()
        for i, v in pairs(game.CoreGui.Hold:GetChildren()) do
            if v.ClassName == "Frame" or v.BackgroundColor3 == Color3.fromRGB(30, 30, 30) then
                game:service'TweenService':Create(v, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
            end
        end        
        for m, x in pairs(game.CoreGui.Hold.Main.Dashboard:GetChildren()) do
            if x.ClassName == "Frame" or x.BackgroundColor3 == Color3.fromRGB(39, 39, 39) then
                game:service'TweenService':Create(x, TweenInfo.new(.3), {BackgroundColor3 = Color3.fromRGB(240, 240, 240)}):Play()
            end
            for l , m in pairs(x:GetDescendants()) do
                if m.ClassName == "TextLabel" and m.TextColor3 == Color3.fromRGB(255, 255, 255) then
                    game:service'TweenService':Create(m, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextColor3 = Color3.fromRGB(0, 0, 0)}):Play()
                end
            end
        end
        for i, v in pairs(game.CoreGui.Hold.Main.TabContentHolder.TabsFolder:GetDescendants()) do
            if v.ClassName == "Frame" or v.ClassName == "TextButton" and v.BackgroundColor3 == Color3.fromRGB(44, 44, 44) then
                print('found: '..v.Name)
                v.BackgroundColor3 = Color3.fromRGB(180, 180, 180)
            end
        end
    end)
    

    Dashboard.Visible = true

    local TabsFolder = Instance.new('Folder')

	TabsFolder.Name = 'TabsFolder'
	TabsFolder.Parent = TabContentHolder

    DashBoardTabButton.MouseButton1Click:Connect(function()
        for i, v in next, TabsFolder:GetChildren() do
            v.Visible = false
        end 
        Dashboard.Visible = true
        for i, v in pairs(SiderBar:GetChildren()) do
            if v.ClassName == "TextButton" and v.BackgroundTransparency == 0 then
                TweenService:Create(v, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
            end
            TweenService:Create(DashBoardTabButton, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
        end
    end)
    
