            Button.TextSize = 14.000
            Button.AutoButtonColor = false
            Button.ClipsDescendants = true

            ButtonCorner.Name = "ButtonCorner"
            ButtonCorner.Parent = Button

            touch_app.Name = "touch_app"
            touch_app.Parent = Button
            touch_app.BackgroundTransparency = 1.000
            touch_app.Position = UDim2.new(0.924528301, 0, 0.24000001, 0)
            touch_app.Size = UDim2.new(0, 25, 0, 25)
            touch_app.ZIndex = 2
            touch_app.Image = "rbxassetid://3926305904"
            touch_app.ImageRectOffset = Vector2.new(84, 204)
            touch_app.ImageRectSize = Vector2.new(36, 36)

            ButtonTitle.Name = Title.."ButtonTitle"
            ButtonTitle.Parent = Button
            ButtonTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ButtonTitle.BackgroundTransparency = 1.000
            ButtonTitle.Position = UDim2.new(0.0245283023, 0, 0.122000001, 0)
            ButtonTitle.Size = UDim2.new(0, 130, 0, 25)
            ButtonTitle.Font = Enum.Font.Gotham
            ButtonTitle.Text = Title
            ButtonTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            ButtonTitle.TextSize = 12.000
            ButtonTitle.TextXAlignment = Enum.TextXAlignment.Left

            ButtonDesc.Name = Title.."ButtonDesc"
            ButtonDesc.Parent = ButtonTitle
            ButtonDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ButtonDesc.BackgroundTransparency = 1.000
            ButtonDesc.Position = UDim2.new(0.00769231096, 0, 0.720000029, 0)
            ButtonDesc.Size = UDim2.new(0, 190, 0, 20)
            ButtonDesc.Font = Enum.Font.Gotham
            ButtonDesc.Text = Desc
            ButtonDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            ButtonDesc.TextSize = 10.000
            ButtonDesc.TextXAlignment = Enum.TextXAlignment.Left

            Sample.Name = "Sample"
            Sample.Parent = Button
            Sample.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Sample.BackgroundTransparency = 1.000
            Sample.Image = "http://www.roblox.com/asset/?id=4560909609"
            Sample.ImageColor3 = Color3.fromRGB(81,81,81)
            Sample.ImageTransparency = 0.600

            local ms = game.Players.LocalPlayer:GetMouse()
            local btn = Button
            local sample = Sample
            Button.MouseButton1Click:Connect(function()
                local c = sample:Clone()
                c.Parent = btn
                local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
                c.Position = UDim2.new(0, x, 0, y)
                local len, size = 0.35, nil
                if btn.AbsoluteSize.X >= btn.AbsoluteSize.Y then
                    size = (btn.AbsoluteSize.X * 1.5)
                else
                    size = (btn.AbsoluteSize.Y * 1.5)
                end
                c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
                for i = 1, 10 do
                    c.ImageTransparency = c.ImageTransparency + 0.05
                    wait(len/ 12)
                end
                pcall(callback)
                c:Destroy()
            end)
            Page.CanvasSize = UDim2.new(0,0,0, List.AbsoluteContentSize.Y)
        end
        function Content:Toggle(Title, Desc, callback)
            local callback = callback or function() end
            local Toggle = Instance.new("Frame")
            local ToggleTitle = Instance.new("TextLabel")
            local ToggleDesc = Instance.new("TextLabel")
            local ToggleCorner = Instance.new("UICorner")
            local Toggle_2 = Instance.new("ImageLabel")
            local ToggleButton = Instance.new("TextButton")
            local ToggleCircle = Instance.new("ImageLabel")
