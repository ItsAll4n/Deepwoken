                    ToggleDesc.Position = UDim2.new(0, 0, 0.99999994, 0)
                    ToggleDesc.Size = UDim2.new(0, 276, 0, 16)
                    ToggleDesc.Font = Enum.Font.Gotham
                    ToggleDesc.Text = Desc
                    ToggleDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
                    ToggleDesc.TextSize = 12.000
                    ToggleDesc.TextXAlignment = Enum.TextXAlignment.Left
                    
                    local toggled = false
                    local debounce = false
                    
                    Button.MouseButton1Click:Connect(function()
                        if debounce == false then
                            if toggled == true then
                                debounce = true
                                game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.25), {ImageColor3 = Color3.fromRGB(200, 200, 200)}):Play()
                                game:GetService("TweenService"):Create(script.Parent.Circle, TweenInfo.new(0.25), {Position = UDim2.new(0,2,0,2)}):Play()
                                wait(0.25)
                                debounce = false
                                toggled = false
                                pcall(callback, false)
                            elseif toggled == false then
                                debounce = true
                                game:GetService("TweenService"):Create(script.Parent, TweenInfo.new(0.25), {ImageColor3 = Color3.fromRGB(64, 200, 114)}):Play()
                                game:GetService("TweenService"):Create(script.Parent.Circle, TweenInfo.new(0.25), {Position = UDim2.new(1,-20,0,2)}):Play()
                                wait(0.25)
                                debounce = false
                                toggled = true
                                pcall(callback, true)
                            end
                        end
                    end)
                end
                local NewSlider = {}
                function NewSlider:Slider(txt, Desc, min, max, callback)
                    local Slider = Instance.new("Frame")
                    local SliderCorner = Instance.new("UICorner")
                    local SliderFrame = Instance.new("Frame")
                    local SliderFrameCorner = Instance.new("UICorner")
                    local SliderButton = Instance.new("TextButton")
                    local InnerColor = Instance.new("Frame")
                    local UICorner_3 = Instance.new("UICorner")
                    local BtnCorner_2 = Instance.new("UICorner")
                    local PercentLabel = Instance.new("TextLabel")
                    local SliderName = Instance.new("TextLabel")
                    local SliderDesc = Instance.new("TextLabel")
                    callback = callback or function() end
                    minvalue = minvalue or 0
                    maxvalue = maxvalue or 100
                    local mouse = game.Players.LocalPlayer:GetMouse()
                    local uis = game:GetService("UserInputService")
                    local Value;
                    
                    Slider.Name = "Slider"
                    Slider.Parent = TabScrollingFrame
                    Slider.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
                    Slider.Position = UDim2.new(0.0128865978, 0, 0.0106382975, 0)
                    Slider.Size = UDim2.new(0, 374, 0, 70)
            
                    SliderCorner.CornerRadius = UDim.new(0, 3)
                    SliderCorner.Name = "SliderCorner"
                    SliderCorner.Parent = Slider
            
                    SliderFrame.Name = "SliderFrame"
                    SliderFrame.Parent = Slider
                    SliderFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    SliderFrame.BackgroundTransparency = 1.000
                    SliderFrame.Position = UDim2.new(0.4467417, 0, 0.619842827, 0)
                    SliderFrame.Size = UDim2.new(0, 198, 0, 7)
            
                    SliderFrameCorner.CornerRadius = UDim.new(0, 99999)
                    SliderFrameCorner.Name = "SliderFrameCorner"
                    SliderFrameCorner.Parent = SliderFrame
            
                    SliderButton.Name = "SliderButton"
                    SliderButton.Parent = SliderFrame
                    SliderButton.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
                    SliderButton.Position = UDim2.new(0, 0, -0.581839442, 0)
                    SliderButton.Size = UDim2.new(0, 193, 0, 11)
                    SliderButton.Font = Enum.Font.SourceSans
                    SliderButton.Text = ""
                    SliderButton.TextColor3 = Color3.fromRGB(0, 0, 0)
                    SliderButton.TextSize = 14.000
            
                    InnerColor.Name = "InnerColor"
                    InnerColor.Parent = SliderButton
                    InnerColor.BackgroundColor3 = Color3.fromRGB(117, 58, 176)
                    InnerColor.BorderSizePixel = 0
                    InnerColor.Position = UDim2.new(0, 0, -0.036543414, 0)
                    InnerColor.Size = UDim2.new(0, 0, 0, 11)
            
                    UICorner_3.CornerRadius = UDim.new(0, 99999)
                    UICorner_3.Parent = InnerColor
            
                    BtnCorner_2.CornerRadius = UDim.new(0, 99999)
                    BtnCorner_2.Name = "BtnCorner"
                    BtnCorner_2.Parent = SliderButton
            
                    PercentLabel.Name = "PercentLabel"
                    PercentLabel.Parent = SliderFrame
                    PercentLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    PercentLabel.BackgroundTransparency = 1.000
                    PercentLabel.Position = UDim2.new(0.728264987, 0, -4.29612494, 0)
                    PercentLabel.Size = UDim2.new(0, 51, 0, 25)
                    PercentLabel.Font = Enum.Font.Gotham
                    PercentLabel.Text = "0"
                    PercentLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
                    PercentLabel.TextSize = 11.000
                    PercentLabel.TextWrapped = true
            
                    SliderName.Name = "SliderName"
                    SliderName.Parent = Slider
                    SliderName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    SliderName.BackgroundTransparency = 1.000
                    SliderName.Position = UDim2.new(0.0348837115, 0, 0.0902300179, 0)
                    SliderName.Size = UDim2.new(0, 138, 0, 33)
                    SliderName.Font = Enum.Font.Gotham
                    SliderName.Text = txt
                    SliderName.TextColor3 = Color3.fromRGB(255, 255, 255)
                    SliderName.TextSize = 13.000
                    SliderName.TextXAlignment = Enum.TextXAlignment.Left
            
                    SliderDesc.Name = "SliderDesc"
                    SliderDesc.Parent = SliderName
                    SliderDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    SliderDesc.BackgroundTransparency = 1.000
                    SliderDesc.Position = UDim2.new(0, 0, 0.99999994, 0)
                    SliderDesc.Size = UDim2.new(0, 276, 0, 16)
                    SliderDesc.Font = Enum.Font.Gotham
                    SliderDesc.Text = Desc
                    SliderDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
                    SliderDesc.TextSize = 12.000
                    SliderDesc.TextXAlignment = Enum.TextXAlignment.Left
                    SliderButton.MouseButton1Down:Connect(function()
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 318) * SliderFrame.AbsoluteSize.X) + tonumber(minvalue)) or 0
                        pcall(function()
                        callback(Value)
                    end)
                    SliderFrame.Size = UDim2.new(0, math.clamp(mouse.X - SliderFrame.AbsolutePosition.X, 0, 318), 0, 16)
                    moveconnection = mouse.Move:Connect(function()
                        PercentLabel.Text = Value
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 318) * SliderFrame.AbsoluteSize.X) + tonumber(minvalue))
                        pcall(function()
                            callback(Value)
                        end)
                        SliderFrame.Size = UDim2.new(0, math.clamp(mouse.X - SliderFrame.AbsolutePosition.X, 0, 318), 0, 16)
                    end)
                    releaseconnection = uis.InputEnded:Connect(function(Mouse)
                        if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                            Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 318) * SliderFrame.AbsoluteSize.X) + tonumber(minvalue))
                            pcall(function()
                                callback(Value)
                            end)
                           SliderFrame.Size = UDim2.new(0, math.clamp(mouse.X - SliderFrame.AbsolutePosition.X, 0, 318), 0, 16)
                            moveconnection:Disconnect()
                            releaseconnection:Disconnect()
                        end
                    end)
                    end)
                    local NewTextBox = {}
                    function NewTextBox:TextBox(text, Desc, Overlay, callback)
                        local TextBox = Instance.new("Frame")
                        local TextBoxCorner = Instance.new("UICorner")
                        local TextBox_2 = Instance.new("TextBox")
                        local Frame = Instance.new("Frame")
                        local TxtName = Instance.new("TextLabel")
                        local TxtDesc = Instance.new("TextLabel")
                        TextBox.Name = "TextBox"
                        TextBox.Parent = TabScrollingFrame
                        TextBox.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
                        TextBox.Size = UDim2.new(0, 374, 0, 70)
                
                        TextBoxCorner.CornerRadius = UDim.new(0, 3)
                        TextBoxCorner.Name = "TextBoxCorner"
                        TextBoxCorner.Parent = TextBox
                
                        TextBox_2.Parent = TextBox
                        TextBox_2.BackgroundColor3 = Color3.fromRGB(2, 2, 2)
                        TextBox_2.BackgroundTransparency = 1.000
                        TextBox_2.Position = UDim2.new(0.505813956, 0, 0.216666669, 0)
                        TextBox_2.Size = UDim2.new(0, 159, 0, 33)
                        TextBox_2.Font = Enum.Font.Gotham
                        TextBox_2.PlaceholderText = Overlay
                        TextBox_2.Text = ""
                        TextBox_2.TextColor3 = Color3.fromRGB(255, 255, 255)
                        TextBox_2.TextSize = 14.000
                        TextBox_2.TextXAlignment = Enum.TextXAlignment.Left
                
                        Frame.Parent = TextBox_2
                        Frame.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
                        Frame.BorderSizePixel = 0
                        Frame.Position = UDim2.new(-0.0251572318, 0, 0.787878811, 0)
                        Frame.Size = UDim2.new(0, 163, 0, 3)
                
                        TxtName.Name = "TxtName"
                        TxtName.Parent = TextBox
                        TxtName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        TxtName.BackgroundTransparency = 1.000
                        TxtName.Position = UDim2.new(0.0348837115, 0, 0.0902300179, 0)
                        TxtName.Size = UDim2.new(0, 138, 0, 33)
                        TxtName.Font = Enum.Font.Gotham
                        TxtName.Text = text
                        TxtName.TextColor3 = Color3.fromRGB(255, 255, 255)
                        TxtName.TextSize = 13.000
                        TxtName.TextXAlignment = Enum.TextXAlignment.Left
                
                        TxtDesc.Name = "TxtDesc"
                        TxtDesc.Parent = TxtName
                        TxtDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        TxtDesc.BackgroundTransparency = 1.000
                        TxtDesc.Position = UDim2.new(0, 0, 0.99999994, 0)
                        TxtDesc.Size = UDim2.new(0, 276, 0, 16)
                        TxtDesc.Font = Enum.Font.Gotham
                        TxtDesc.Text = Desc
                        TxtDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
                        TxtDesc.TextSize = 12.000
                        TxtDesc.TextXAlignment = Enum.TextXAlignment.Left
                        TextBox.FocusLost:Connect(function(ep)
                            if ep then
                                if #TextBox.Text > 0 then
                                    pcall(callback, TextBox.Text)
                                end
                            end
                        end)
                    return NewTextBox
                    end
                   return NewSlider
                end
               return NewToggle
             end
            return NewButton
        end
    return NewTab
end
return Lib
