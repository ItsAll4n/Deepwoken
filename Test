        function Content:Button(Title, Desc, callback)
            local callback = callback or function() end
            local Button = Instance.new("TextButton")
            local ButtonCorner = Instance.new("UICorner")
            local touch_app = Instance.new("ImageLabel")
            local ButtonTitle = Instance.new("TextLabel")
            local ButtonDesc = Instance.new("TextLabel")
            local Sample = Instance.new("ImageLabel")

            Button.Name = Title.."Button"
            Button.Parent = Page
            Button.BackgroundColor3 = Color3.fromRGB(180, 180, 180)
            Button.BorderSizePixel = 0
            Button.Position = UDim2.new(0.0255009104, 0, 0.139423072, 0)
            Button.Size = UDim2.new(0, 530, 0, 57)
            Button.Font = Enum.Font.SourceSans
