        local NewTab = {}

        function NewTab:Tab(txt)
            local TabButton = Instance.new("TextButton")
            local TabBtnCorner = Instance.new("UICorner")
            local TabScrollingFrame = Instance.new("ScrollingFrame")
            local TabHolderListLayOut = Instance.new("UIListLayout")

            TabButton.Name = txt.."TabButton"
            TabButton.Parent = ScrollingFrame
            TabButton.BackgroundColor3 = Color3.fromRGB(61, 61, 61)
            TabButton.Position = UDim2.new(0.0614525154, 0, 0.0200574212, 0)
            TabButton.Size = UDim2.new(0, 157, 0, 37)
            TabButton.Font = Enum.Font.Gotham
            TabButton.Text = txt
            TabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            TabButton.TextSize = 14.000
            TabButton.AutoButtonColor = false
            
            TabButton.MouseButton1Click:Connect(function()
            for i,v in next, TabFolder:GetChildren() do 
                v.Visible = false   
            end
            TabScrollingFrame.Visible = true

            for i,v in next, ScrollingFrame:GetChildren() do   
                if v:IsA("TextButton") then
                    game.TweenService:Create(v, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
                        BackgroundColor3 = Color3.fromRGB(61, 61, 61) 
                    }):Play()
                end
            end
            end)

            TabBtnCorner.Parent = TabButton
            TabBtnCorner.Name = txt.."TabBtnCorner"
            TabBtnCorner.CornerRadius = UDim.new(0, 4)
            
            TabHolderListLayOut.Parent = ScrollingFrame
            TabHolderListLayOut.Name = txt.."ListLayOut"
            TabHolderListLayOut.HorizontalAlignment = Enum.HorizontalAlignment.Center
            TabHolderListLayOut.SortOrder = Enum.SortOrder.LayoutOrder
            TabHolderListLayOut.Padding = UDim.new(0, 5)
            

            TabScrollingFrame.Name = "TabScrollingFrame"
            TabScrollingFrame.Parent = Tab
            TabScrollingFrame.Active = true
            TabScrollingFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TabScrollingFrame.BackgroundTransparency = 1.000
            TabScrollingFrame.BorderSizePixel = 0
            TabScrollingFrame.Position = UDim2.new(-0.00268588401, 0, -0.0255319141, 0)
            TabScrollingFrame.Size = UDim2.new(0, 388, 0, 268)
            TabScrollingFrame.ScrollBarThickness = 4
            
